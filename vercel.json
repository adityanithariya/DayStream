{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**", "node_modules/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "*": {
      "maxDuration": 60
    }
  }
}
